{
  "identifier": "mobile",
  "description": "Additional capabilities for mobile platforms",
  "platforms": ["iOS", "android"],
  "permissions": [
    "core:default",
    "opener:default",
    "notification:default",
    {
      "identifier": "fs:default",
      "allow": [
        { "path": "$APPDATA/**" },
        { "path": "$APPDATA/images/**" }
      ]
    }
  ]
}
