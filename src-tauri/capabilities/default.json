{
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "opener:default",
    "notification:default",
    {
      "identifier": "fs:default",
      "allow": [
        { "path": "$APPDATA/**" },
        { "path": "$APPDATA/images/**" }
      ]
    }
  ]
}
